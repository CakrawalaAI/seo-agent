  "scripts": {
    "dev": "bunx vite dev --host",
    "build": "bunx vite build",
    "start": "NODE_ENV=production bunx vite preview",
    "lint": "bunx biome check .",
    "lint:fix": "bunx biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "db:generate": "bunx drizzle-kit generate",
    "db:prepare": "bunx drizzle-kit up",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:reset": "bunx tsx scripts/db-reset.ts && bun run db:prepare && bun run db:migrate",
    "shadcn": "bunx shadcn-ui@latest",
    "polar:setup": "bunx tsx scripts/polar-setup.ts",
    "worker": "tsx src/worker/index.ts",
    "worker:build": "bun build src/worker/index.ts --outdir dist/worker --target bun --format esm --entry-naming [name].mjs --external playwright --external playwright-core --external electron --external chromium-bidi/lib/cjs/bidiMapper/BidiMapper --external chromium-bidi/lib/cjs/cdp/CdpConnection",
    "worker:test": "vitest run src/worker",
    "smoke": "bunx tsx scripts/smoke.ts",
    "smoke:project": "E2E_NO_AUTH=1 bunx tsx scripts/smoke-project.ts",
    "e2e": "playwright test"
  },
