{
  "name": "seo-agent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "seo": "dist/cli/index.mjs"
  },
  "packageManager": "bun@1.3.0",
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "bunx vite dev --host",
    "build": "bunx vite build",
    "start": "NODE_ENV=production bunx vite preview",
    "lint": "bunx biome check .",
    "lint:fix": "bunx biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "shadcn": "bunx shadcn-ui@latest",
    "cli:dev": "bunx tsx src/cli/index.ts",
    "cli:build": "bun build src/cli/index.ts --outdir dist/cli --target node --format esm --entry-naming [name].mjs --external ora --external commander",
    "worker:dev": "bunx tsx src/worker/index.ts",
    "worker:build": "bun build src/worker/index.ts --outdir dist/worker --target bun --format esm --entry-naming [name].mjs --external playwright --external playwright-core --external electron --external chromium-bidi/lib/cjs/bidiMapper/BidiMapper --external chromium-bidi/lib/cjs/cdp/CdpConnection",
    "worker:test": "vitest run src/worker",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/devtools-vite": "^0.3.10",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.15",
    "@tanstack/react-router-ssr-query": "^1.133.16",
    "@tanstack/react-start": "^1.133.15",
    "@tanstack/router-devtools": "^1.133.15",
    "@tiptap/pm": "^2.6.4",
    "@tiptap/react": "^2.6.4",
    "@tiptap/starter-kit": "^2.6.4",
    "@vitejs/plugin-react": "^5.0.4",
    "amqplib": "^0.10.3",
    "autoprefixer": "^10.4.19",
    "better-auth": "^1.3.28",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "eventemitter3": "^5.0.1",
    "fast-xml-parser": "^4.3.6",
    "lucide-react": "^0.546.0",
    "nitro": "^3.0.1-alpha.0",
    "openai": "^4.47.3",
    "p-retry": "^6.2.0",
    "playwright": "^1.45.0",
    "postcss": "^8.4.49",
    "postgres": "^3.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "robots-parser": "^3.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.14",
    "undici": "^6.19.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tanstack/eslint-plugin-router": "^1.133.3",
    "@tanstack/router-generator": "^1.133.15",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@types/amqplib": "^0.10.5",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "@types/tapable": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.5",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^1.6.0"
  }
}
