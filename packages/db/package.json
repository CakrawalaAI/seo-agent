{
  "name": "@seo-agent/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "drizzle.config.ts"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "migrate": "drizzle-kit migrate",
    "generate": "drizzle-kit generate",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@seo-agent/domain": "file:../domain",
    "drizzle-orm": "^0.44.6",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "drizzle-kit": "^0.24.2",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/schema.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "clean": true,
    "external": [
      "@seo-agent/domain",
      "drizzle-orm",
      "postgres"
    ]
  }
}
