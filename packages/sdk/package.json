{
  "name": "@seo-agent/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@seo-agent/domain": "file:../domain",
    "undici": "^6.19.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/client.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": false,
    "clean": true,
    "dts": true
  }
}
